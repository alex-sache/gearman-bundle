{
  "name": "eduardtrandafir/gearman-bundle",
  "replace": {
    "eduardtrandafir/gearman-bundle": "dev-master"
  },
  "description": "Adds gearman support to your Symfony3 project",
  "keywords": ["gearman"],
  "homepage": "https://github.com/eduardtrandafir/gearman-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Eduard Trandafir",
      "email": "edytrandafir@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "ext-gearman": "*",

    "doctrine/doctrine-cache-bundle": "~1.0",
    "doctrine/common": "~2.2",

    "symfony/dependency-injection": "~3.0",
    "symfony/framework-bundle": "~3.0",
    "symfony/http-kernel": "~3.0",
    "symfony/console": "~3.0",
    "symfony/config": "~3.0",
    "symfony/finder": "~3.0",
    "symfony/options-resolver": "~3.0"
  },
  "target-dir": "EduardTrandafir/GearmanBundle",
  "autoload": {
    "psr-0": { "EduardTrandafir\\GearmanBundle": "" }
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8"
  }
}